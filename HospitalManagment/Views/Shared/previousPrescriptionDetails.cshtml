@model IEnumerable<HospitalManagment.Models.Prescription>

 
<table   border="1">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OpdID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameOfMedicine)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dosage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeOfMedicine)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Weight)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfDays)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ManagementPlan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.newmedicine)
        </th>
        <th>
