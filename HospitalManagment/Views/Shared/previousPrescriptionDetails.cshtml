    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OpdID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameOfMedicine)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dosage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TypeOfMedicine)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Weight)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberOfDays)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ManagementPlan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.newmedicine)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TypeOfIntakeAdv)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NextOpointmentDate)
        </td>
        
    </tr>
}

</table>
